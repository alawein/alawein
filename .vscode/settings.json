    "sort *",
    "uniq *",
    "wc *",
    "tar *",
    "zip *",
    "unzip *",
    "gzip *",
    "gunzip *",
    "ssh *",
    "scp *",
    "rsync *",
    "systemctl *",
    "service *",
    "ps *",
    "top *",
    "htop *",
    "kill *",
    "killall *",
    "nohup *",
    "screen *",
    "tmux *",
    "vim *",
    "nano *",
    "emacs *",
    "code *",
    "npm run lint 2>&1 | Select-Object -First 50",
    "npm run type-check",
    "npm run lint",
    "npm run lint:fix",
    "npm run format:check",
    "npm run test:run",
    "npm audit",
    "npm run *",
    "yarn run *",
    "pnpm run *"
