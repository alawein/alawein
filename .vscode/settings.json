    "go *",
    "cargo *",
    "rustc *",
    "javac *",
    "java *",
    "gcc *",
    "g++ *",
    "clang *",
    "make *",
    "cmake *",
    "pip *",
    "pip3 *",
    "conda *",
    "brew *",
