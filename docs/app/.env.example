# Studios Hub Environment Variables
# Copy this file to .env and fill in the values

# App Configuration
VITE_APP_NAME=studios-hub
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=development

# Sentry Error Tracking
# Get your DSN from: https://sentry.io/settings/projects/studios-hub/keys/
VITE_SENTRY_DSN=

# Analytics (Plausible)
VITE_PLAUSIBLE_DOMAIN=alawein.dev

# Feature Flags (optional)
VITE_ENABLE_ANALYTICS_DASHBOARD=true

