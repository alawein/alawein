{
  "name": "meta-governance",
  "version": "1.0.0",
  "description": "Meta-governance repository with DevOps template library and CLI tools",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "devops": "tsx tools/cli/devops.ts",
    "devops:list": "tsx tools/cli/devops.ts template list",
    "devops:builder": "tsx tools/cli/devops.ts template apply",
    "devops:coder:dry": "tsx tools/cli/devops.ts generate node-service --dry-run",
    "devops:coder": "tsx tools/cli/devops.ts generate node-service",
    "devops:bootstrap": "tsx tools/cli/devops.ts init",
    "devops:setup": "tsx tools/cli/devops.ts setup",
    "governance": "python tools/cli/governance.py",
    "orchestrate": "python tools/cli/orchestrate.py",
    "mcp": "python tools/cli/mcp.py",
    "automation": "tsx automation/cli/index.ts",
    "automation:list": "tsx automation/cli/index.ts list",
    "automation:execute": "tsx automation/cli/index.ts execute",
    "automation:route": "tsx automation/cli/index.ts route",
    "automation:run": "tsx automation/cli/index.ts run",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "health": "tsx tools/health/repo-health.ts",
    "health:ci": "tsx tools/health/repo-health.ts --ci",
    "prepare": "husky",
    "ORCHEX": "tsx tools/ORCHEX/cli/index.ts",
    "ORCHEX:api": "tsx tools/ORCHEX/api/cli.ts",
    "ORCHEX:api:start": "tsx tools/ORCHEX/api/cli.ts",
    "ORCHEX:storage:migrate": "tsx tools/ORCHEX/storage/migrate.ts",
    "ai": "tsx tools/ai/index.ts",
    "ai:tools": "tsx tools/ai/index.ts",
    "ai:start": "tsx tools/ai/orchestrator.ts start",
    "ai:complete": "tsx tools/ai/orchestrator.ts complete",
    "ai:context": "tsx tools/ai/orchestrator.ts context",
    "ai:metrics": "tsx tools/ai/orchestrator.ts metrics",
    "ai:history": "tsx tools/ai/orchestrator.ts history",
    "ai:sync": "tsx tools/ai/sync.ts",
    "ai:dashboard": "tsx tools/ai/dashboard.ts",
    "ai:cache": "tsx tools/ai/cache.ts",
    "ai:cache:stats": "tsx tools/ai/cache.ts stats",
    "ai:cache:clear": "tsx tools/ai/cache.ts clear",
    "ai:monitor": "tsx tools/ai/monitor.ts",
    "ai:monitor:status": "tsx tools/ai/monitor.ts status",
    "ai:monitor:check": "tsx tools/ai/monitor.ts check",
    "ai:compliance": "tsx tools/ai/cli/compliance-cli.ts",
    "ai:compliance:check": "tsx tools/ai/cli/compliance-cli.ts check",
    "ai:compliance:score": "tsx tools/ai/cli/compliance-cli.ts score",
    "ai:telemetry": "tsx tools/ai/telemetry.ts",
    "ai:telemetry:status": "tsx tools/ai/telemetry.ts status",
    "ai:telemetry:alerts": "tsx tools/ai/telemetry.ts alerts",
    "ai:errors": "tsx tools/ai/errors.ts",
    "ai:errors:list": "tsx tools/ai/errors.ts list",
    "ai:errors:stats": "tsx tools/ai/errors.ts stats",
    "ai:security": "tsx tools/ai/cli/security-cli.ts",
    "ai:security:scan": "tsx tools/ai/cli/security-cli.ts scan",
    "ai:security:secrets": "tsx tools/ai/cli/security-cli.ts secrets",
    "ai:security:vulns": "tsx tools/ai/cli/security-cli.ts vulns",
    "ai:issues": "tsx tools/ai/cli/issues-cli.ts",
    "ai:issues:list": "tsx tools/ai/cli/issues-cli.ts list",
    "ai:issues:critical": "tsx tools/ai/cli/issues-cli.ts critical",
    "ai:issues:stats": "tsx tools/ai/cli/issues-cli.ts stats",
    "ai:mcp": "tsx tools/ai/mcp/server.ts",
    "ai:mcp:start": "tsx tools/ai/mcp/server.ts start",
    "ai:mcp:tools": "tsx tools/ai/mcp/server.ts tools",
    "ai:mcp:resources": "tsx tools/ai/mcp/server.ts resources",
    "ai:api": "tsx tools/ai/api/server.ts",
    "ai:api:start": "tsx tools/ai/api/server.ts start",
    "ai:tokens": "tsx tools/ai/tokens.ts",
    "ai:tokens:stats": "tsx tools/ai/tokens.ts stats",
    "ai:tokens:reset": "tsx tools/ai/tokens.ts reset session",
    "ai:tier": "tsx tools/ai/tokens.ts tier",
    "ai:proxy": "tsx tools/ai/proxy/server.ts",
    "ai:proxy:start": "tsx tools/ai/proxy/server.ts start",
    "ai:guide": "tsx tools/ai/proxy/guidance.ts",
    "ai:guide:start": "tsx tools/ai/proxy/guidance.ts start",
    "ai:guide:end": "tsx tools/ai/proxy/guidance.ts end",
    "ai:guide:quick": "tsx tools/ai/proxy/guidance.ts quick",
    "db": "tsx tools/ai/utils/database.ts",
    "db:init": "tsx tools/ai/utils/database.ts init",
    "db:migrate": "tsx tools/ai/utils/database.ts migrate",
    "db:stats": "tsx tools/ai/utils/database.ts stats",
    "security:audit": "tsx security-audit.ts",
    "security:scan": "npm run security:audit",
    "security:dependencies": "npm audit --audit-level=moderate",
    "security:secrets": "detect-secrets scan --baseline .secrets.baseline --all-files",
    "perf:bundle": "tsx bundle-analyzer.ts",
    "perf:python": "python python-benchmark.py",
    "telemetry": "tsx tools/telemetry/index.ts",
    "telemetry:dashboard": "tsx tools/telemetry/index.ts dashboard",
    "health:services": "tsx tools/health/check.ts",
    "backup": "tsx tools/backup/backup.ts",
    "backup:configs": "tsx tools/backup/backup.ts configs",
    "backup:list": "tsx tools/backup/backup.ts list",
    "a11y:audit": "tsx tools/accessibility/audit.ts audit",
    "a11y:check": "tsx tools/accessibility/audit.ts checklist"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/better-sqlite3": "7.6.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "22.19.1",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.15.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.1",
    "tsx": "^4.19.2",
    "turbo": "^1.10.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vitest": "3.2.4"
  },
  "lint-staged": {
    "!(templates/**/*)*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(templates/**/*)*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "devops",
    "templates",
    "governance",
    "cli",
    "kubernetes",
    "helm",
    "ci-cd"
  ],
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "12.5.0",
    "chalk": "5.6.2",
    "chokidar": "^5.0.0",
    "commander": "14.0.2",
    "ora": "9.0.0",
    "ws": "8.18.3",
    "yaml": "^2.8.2"
  }
}
