{
  "name": "@alawein/integrations",
  "version": "1.0.0",
  "description": "Shared integration clients for Supabase, Stripe, Resend, Sentry across Alawein monorepo",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./supabase": {
      "import": "./dist/supabase/index.js",
      "types": "./dist/supabase/index.d.ts"
    },
    "./supabase/client": {
      "import": "./dist/supabase/client.js",
      "types": "./dist/supabase/client.d.ts"
    },
    "./supabase/auth": {
      "import": "./dist/supabase/auth.js",
      "types": "./dist/supabase/auth.d.ts"
    },
    "./stripe": {
      "import": "./dist/stripe/index.js",
      "types": "./dist/stripe/index.d.ts"
    },
    "./stripe/checkout": {
      "import": "./dist/stripe/checkout.js",
      "types": "./dist/stripe/checkout.d.ts"
    },
    "./stripe/subscriptions": {
      "import": "./dist/stripe/subscriptions.js",
      "types": "./dist/stripe/subscriptions.d.ts"
    },
    "./resend": {
      "import": "./dist/resend/index.js",
      "types": "./dist/resend/index.d.ts"
    },
    "./sentry": {
      "import": "./dist/sentry/index.js",
      "types": "./dist/sentry/index.d.ts"
    },
    "./analytics": {
      "import": "./dist/analytics/index.js",
      "types": "./dist/analytics/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "integrations",
    "supabase",
    "stripe",
    "resend",
    "sentry",
    "vercel"
  ],
  "dependencies": {
    "@supabase/supabase-js": "^2.47.0",
    "@stripe/stripe-js": "^4.10.0",
    "resend": "^4.0.0",
    "@sentry/react": "^8.0.0",
    "@vercel/analytics": "^1.4.0",
    "@vercel/speed-insights": "^1.1.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^22.10.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}
