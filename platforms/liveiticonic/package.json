{
  "name": "live-it-iconic",
  "displayName": "Live It Iconic",
  "description": "Premium lifestyle merchandise for luxury automotive enthusiasts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "keywords": [
    "luxury merchandise",
    "premium apparel",
    "automotive lifestyle",
    "lifestyle brand",
    "luxury apparel"
  ],
  "author": "Live It Iconic",
  "license": "Proprietary",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "ai:images": "tsx scripts/ai/generate-images.ts",
    "data:market": "python scripts/data/market_signals.py",
    "export:site": "node scripts/export/export-site.mjs",
    "workflow:execute": "tsx scripts/workflow/workflow-engine.ts execute",
    "workflow:status": "tsx scripts/workflow/workflow-engine.ts status",
    "workflow:reset": "tsx scripts/workflow/workflow-engine.ts reset",
    "setup:dev": "tsx scripts/setup/dev-environment.ts setup",
    "setup:check": "tsx scripts/setup/dev-environment.ts check",
    "brand:validate": "tsx scripts/brand/asset-validator.ts validate",
    "brand:report": "tsx scripts/brand/asset-validator.ts report",
    "brand:animations": "tsx scripts/brand/animation-generator.ts",
    "brand:images": "tsx scripts/brand/image-processor.ts manifest",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "launch": "tsx src/launch-platform/cli/launch-cli.ts",
    "launch:demo": "tsx src/launch-platform/examples/demo-launch.ts",
    "launch:examples": "tsx src/launch-platform/examples/basic-agent-usage.ts",
    "launch:agents": "tsx src/launch-platform/cli/launch-cli.ts agents",
    "launch:stats": "tsx src/launch-platform/cli/launch-cli.ts stats",
    "launch:monitor": "tsx src/launch-platform/examples/monitoring-demo.ts",
    "launch:dashboard": "tsx src/launch-platform/examples/monitoring-demo.ts live",
    "perf:verify": "tsx scripts/performance/verify-optimizations.ts",
    "perf:audit": "npm run build && npm run preview",
    "perf:lighthouse": "npx lighthouse http://localhost:4173/ --output=json --output=html --output-path=./perf-reports/lighthouse --view",
    "email:preview": "react-email dev --dir src/emails",
    "email:export": "react-email export --dir src/emails",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "storybook build && npx http-server storybook-static",
    "build-storybook": "storybook build",
    "backup:create": "tsx scripts/backup/database-backup.ts",
    "backup:list": "tsx scripts/backup/database-backup.ts list",
    "backup:restore": "tsx scripts/backup/database-restore.ts",
    "backup:validate": "tsx scripts/backup/database-restore.ts validate",
    "backup:start": "tsx scripts/backup/automated-backup.ts once",
    "backup:daemon": "tsx scripts/backup/automated-backup.ts daemon",
    "backup:stats": "tsx scripts/backup/automated-backup.ts stats",
    "backup:download-s3": "tsx scripts/backup/cloud-storage.ts download"
  },
