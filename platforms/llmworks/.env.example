# Environment Variables for LLM Works
# CRITICAL SECURITY: Set these in production!

# API Configuration (optional)
VITE_API_URL=
VITE_CDN_URL=
VITE_WS_URL=

# CRITICAL SECURITY: API Key Encryption
# MUST be set in production - generate with: openssl rand -hex 32
# WARNING: Using default key is a security vulnerability
VITE_API_KEY_ENCRYPTION_SALT=

# Authentication Security
# Set your Supabase project URL and anon key
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Notifications (optional)
VITE_VAPID_PUBLIC_KEY=

# Monitoring (optional)
VITE_SENTRY_DSN=

# Environment (auto-detected)
# Options: development, staging, production
VITE_ENVIRONMENT=