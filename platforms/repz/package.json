    "react-hook-form": "^7.53.0",
    "react-i18next": "^16.3.1",
    "react-resizable-panels": "^3.0.6",
    "react-router-dom": "^6.26.2",
    "recharts": "^3.1.0",
    "sonner": "^1.5.0",
