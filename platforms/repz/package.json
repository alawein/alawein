{
  "name": "repz-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8080",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview --port 8080",
    "preview:staging": "vite preview --port 8080 --mode staging",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/",
    "test:component": "vitest run src/components/",
    "test:integration": "vitest run tests/integration/",
    "type-check": "tsc --noEmit",
    "rollback": "tsx scripts/rollback-manager.ts",
    "refactor:metrics": "tsx scripts/component-consolidator.js",
    "refactor:analyze": "tsx scripts/refactor-analyzer.ts",
    "analyze:bundle": "vite build --mode production && npx vite-bundle-analyzer dist/assets",
    "analyze:deps": "tsx scripts/dependency-analyzer.ts",
    "audit:quick": "node scripts/quick-audit.cjs",
    "audit:routes": "node scripts/audit-dead-pages.cjs",
    "audit:full": "npm run audit:quick && npm run audit:routes",
    "validate:production": "npm run type-check && npm run audit:full && npm run build:production",
    "validate:routes": "node scripts/quick-audit.cjs --routes-only",
    "validate:env": "node -e \"console.log('Environment validation - implement as needed')\"",
    "db:status": "echo 'Database commands removed - using Vercel instead'",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:a11y": "playwright test tests/accessibility/",
    "test:performance": "npm run test:performance:smoke && npm run test:performance:load",
    "test:performance:smoke": "k6 run tests/performance/smoke-test.js",
    "test:performance:load": "k6 run tests/performance/load-test.js",
    "test:performance:stress": "k6 run tests/performance/stress-test.js",
    "test:security": "npm audit --audit-level=moderate && echo 'Run OWASP ZAP scan manually'",
    "test:all": "npm run lint && npm run type-check && npm run test:run && npm run test:e2e && npm run test:a11y",
    "test:auth": "npm run test -- --testNamePattern='auth|login|signup'",
    "test:stripe": "npm run test -- --testNamePattern='stripe|payment'",
    "test:monitoring": "curl -f http://localhost:8080/api/health || echo 'Health check endpoint not available'",
    "test:responsive": "echo 'Responsive design tests - implement with Percy or similar'",
    "test:infrastructure": "echo 'Infrastructure tests - implement based on hosting platform'",
    "test:production-smoke": "echo 'Production smoke tests - implement critical path validation'",
    "prepare:rollback": "git tag rollback-$(date +%Y%m%d-%H%M%S) && echo 'Rollback point tagged'",
    "validate:prod-env": "echo 'Production environment validation - implement environment checks'",
    "agents:cli": "tsx scripts/agents-cli.ts",
    "agents:aggregate": "tsx scripts/agents-aggregate.ts",
    "deploy:production": "./scripts/deploy-production.sh",
    "deploy:setup": "./scripts/setup-integrations.sh",
    "deploy:check": "npm run type-check && npm run build:production",
    "deploy:health": "./scripts/health-check.sh",
    "deploy:cost": "./scripts/cost-calculator.sh",
    "vercel:env": "vercel env pull .env.production",
    "vercel:logs": "vercel logs --prod",
    "vercel:deploy": "vercel --prod"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@axe-core/react": "^4.11.0",
