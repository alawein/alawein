{
  "name": "@alaweinos/simcore",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest -c vite.config.test.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "push": "git add . && git commit -m \"chore: deploy\" || echo No changes to commit && git push origin main",
    "deploy": "npm run build",
    "deploy:replit": "npm run push",
    "start": "npm run build && npx serve -s dist -l ${PORT:-3000} -n",
    "audit:prep": "mkdir -p .audit .audit/lhci .audit/axe .audit/playwright .audit/css || true",
    "audit:lh": "lhci autorun",
    "audit:axe": "node ./scripts/audit-axe.mjs",
    "audit:pw": "playwright test --config=./scripts/playwright.audit.config.ts",
    "audit:ai": "node scripts/audit-ai-visual.js",
    "audit:css": "npx --yes analyze-css dist/assets/*.css --pretty --json > .audit/css.json || npx --yes analyze-css dist/**/*.css --pretty --json > .audit/css.json || echo {} > .audit/css.json",
    "audit:pa11y": "npx --yes pa11y http://127.0.0.1:4173 --reporter json > .audit/pa11y.json",
    "audit:full": "npm run audit:prep && start-server-and-test preview http://127.0.0.1:4173 \"npm run audit:lh && npm run audit:axe && npm run audit:pw && npm run audit:css && npm run audit:pa11y\"",
    "audit:ci": "npm run audit:prep && start-server-and-test preview http://127.0.0.1:4173 \"npm run audit:lh && npm run audit:axe && npm run audit:pw && npm run audit:css && npm run audit:pa11y\""
  },
