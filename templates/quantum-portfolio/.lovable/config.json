{
  "$schema": "https://lovable.dev/schemas/config.json",
  "name": "quantum-portfolio",
  "description": "Quantum-themed developer portfolio with orbital particles, vaporwave aesthetics, and retro-futuristic design",
  "version": "1.0.0",
  "template": "react-vite",
  "theme": "quantum",
  "boundaries": {
    "lovable_owned": [
      "src/components/ui/**",
      "src/components/layout/**",
      "src/pages/**"
    ],
    "local_owned": [
      "src/services/**",
      "src/stores/**",
      "src/hooks/**",
      "src/types/**",
      "src/lib/**"
    ],
    "shared": [
      "src/components/effects/**",
      "src/components/shared/**"
    ]
  },
  "features": {
    "routing": true,
    "state_management": "zustand",
    "styling": "tailwind",
    "animations": "framer-motion",
    "icons": "lucide-react"
  },
  "deployment": {
    "platform": "vercel",
    "build_command": "npm run build",
    "output_directory": "dist"
  }
}

