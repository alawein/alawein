{
  "name": "saas-dashboard",
  "description": "Modern SaaS admin dashboard with analytics, user management, and settings",
  "version": "1.0.0",
  "template": true,
  "framework": "react",
  "styling": "tailwindcss",
  "features": [
    "dashboard-analytics",
    "user-management",
    "settings-panel",
    "dark-mode",
    "responsive-sidebar"
  ],
  "boundaries": {
    "lovable_owned": [
      "src/components/ui/**",
      "src/components/charts/**",
      "src/components/layout/Sidebar.tsx",
      "src/components/layout/Header.tsx"
    ],
    "local_owned": [
      "src/pages/**",
      "src/hooks/**",
      "src/lib/**",
      "src/stores/**",
      "tailwind.config.js",
      "vite.config.ts"
    ]
  },
  "theme": {
    "primary": "blue",
    "style": "professional",
    "radius": "0.5rem"
  }
}

